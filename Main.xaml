                            </ActivityAction.Argument>
                            <Sequence DisplayName="Body" sap2010:WorkflowViewState.IdRef="Sequence_13">
                              <Assign sap2010:WorkflowViewState.IdRef="Assign_10">
                                <Assign.To>
