                        </Assign.Value>
                      </Assign>
                      <ui:ForEach x:TypeArguments="x:Object" DisplayName="For each" sap2010:WorkflowViewState.IdRef="ForEach`1_3" Values="[readColumn]">
                        <ui:ForEach.Body>
